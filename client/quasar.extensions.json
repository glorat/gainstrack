{
  "@quasar/dotenv": {
    "env_development": ".env.development",
    "env_production": ".env.production",
    "common_root_object": "none",
    "create_env_files": false,
    "add_env_to_gitignore": false
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest@alpha"
    ]
  },
  "@quasar/testing-unit-jest": {
    "options": [
      "scripts",
      "typescript"
    ]
  }
}